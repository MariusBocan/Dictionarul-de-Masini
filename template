// Sign up/ Log in

  const conturi = {
    nume: "",
    email: "",
    parola: "",
  };
  
  const conturiArr = [];
  
  $(document).ready(function () {
    $("#inButon").click(function () {
      alert($("#inParola").val());
  
      conturi.nume = $("#inNume").val();
      conturi.email = $("#inEmail").val();
      conturi.parola = $("#inParola").val();
  
      conturiArr.push($("#inNume").val());
      conturiArr.push($("#inEmail").val());
      conturiArr.push($("#inParola").val());
  
      alert(conturiArr);
  
      $("#inNume").val("");
      $("#inEmail").val("");
      $("#inParola").val("");
    });
  
    $("#userButon").click(function () {
      if (
        $("#userEmail").val() == conturi.email &&
        $("#userParola").val() == conturi.parola
      ) {
        alert("Datele de logare is corecte!");
        window.location.href = "profile.html";
      } else {
        alert("Datele de logare is gresite!");
      }
    });
  
    // Modificarea pozei de profil
  
    $("#b9").click(function () {
      $("#img1").attr("src", $("#in0").val());
      $("#in0").val("");
    });
  
    // Evenimentul de completare a datelor personale in pagina de utilizator
  
    $("#btnsave").click(function () {
      $("#p1").text("Numele       : " + $("#in1").val());
      $("#p2").text("Prenumele    : " + $("#in2").val());
      $("#p3").text("Data nasterii: " + $("#in3").val());
      $("#p4").text("Email        : " + $("#inemail4").val());
  
      $("#in1").val("");
      $("#in2").val("");
      $("#in3").val("");
      $("#inemail4").val("");
    });
  
    // Evenimentul de click pe data curenta
  
    // Evenimentul de revenire pe index.html
  
    $("#btncancel").click(function () {
      window.location.href = "index.html";
    });
  });
  